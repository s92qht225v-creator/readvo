            { "i": [1, 2], "p": "zài", "t": "-da", "tr": "в", "h": 1, "l": 4 },
            { "i": [2, 4], "p": "jiàoshì", "t": "sinf", "tr": "класс" },
            { "i": [4, 5], "p": "ma", "t": "-mi", "tr": "(вопрос)", "h": 1, "l": 2 }
