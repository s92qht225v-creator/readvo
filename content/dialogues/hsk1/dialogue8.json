            { "i": [1, 2], "p": "jǐ", "t": "necha", "tr": "сколько", "h": 1, "l": 6 },
            { "i": [2, 3], "p": "diǎn", "t": "soat", "tr": "час", "h": 1, "l": 6 },
            { "i": [3, 4], "p": "huí", "t": "qaytmoq", "tr": "вернуться", "h": 1, "l": 7 },
            { "i": [4, 5], "p": "jiā", "t": "uy", "tr": "дом", "h": 1, "l": 7 }
          ]
        },
        {
          "id": "d8-s6",
          "text_original": "五点回家。",
          "pinyin": "Wǔ diǎn huí jiā.",
          "text_translation": "Soat beshda qaytaman.",
