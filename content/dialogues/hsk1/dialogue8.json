            { "i": [2, 3], "p": "huí", "t": "qaytmoq", "tr": "вернуться", "h": 1, "l": 7 },
            { "i": [3, 4], "p": "jiā", "t": "uy", "tr": "дом", "h": 1, "l": 7 }
          ]
        },
        {
          "id": "d8-s7",
          "text_original": "好，我等你。",
          "pinyin": "Hǎo, wǒ děng nǐ.",
