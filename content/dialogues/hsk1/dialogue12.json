{
  "id": "hsk1-dialogue12",
  "title": "我不舒服",
  "pinyin": "Wǒ bù shūfu",
  "titleTranslation": "O'zimni yomon his qilyapman",
  "titleTranslation_ru": "Мне нехорошо",
  "level": 1,
  "sections": [
    {
      "id": "d12-sec1",
      "type": "text",
      "heading": "",
      "subheading": "",
      "sentences": [
        {
          "id": "d12-s1",
          "text_original": "你怎么了？",
          "pinyin": "Nǐ zěnme le?",
          "text_translation": "Senga nima bo'ldi?",
          "text_translation_ru": "Что с тобой?",
          "speaker": "A",
          "words": [
            { "i": [0, 1], "p": "nǐ", "t": "sen", "tr": "ты", "h": 1, "l": 1 },
            { "i": [1, 3], "p": "zěnme", "t": "qanday/nima", "tr": "как/что", "h": 1, "l": 7 },
            { "i": [3, 4], "p": "le", "t": "(o'tgan zamon)", "tr": "(прош. время)", "h": 1, "l": 8 }
          ]
        },
        {
          "id": "d12-s2",
          "text_original": "我不舒服。",
          "pinyin": "Wǒ bù shūfu.",
          "text_translation": "O'zimni yomon his qilyapman.",
          "text_translation_ru": "Мне нехорошо.",
          "speaker": "B",
          "words": [
            { "i": [0, 1], "p": "wǒ", "t": "men", "tr": "я", "h": 1, "l": 1 },
            { "i": [1, 2], "p": "bù", "t": "emas", "tr": "не", "h": 1, "l": 2 },
            { "i": [2, 4], "p": "shūfu", "t": "qulay/yaxshi", "tr": "комфортно" }
          ]
        },
        {
          "id": "d12-s3",
          "text_original": "你哪儿不舒服？",
          "pinyin": "Nǐ nǎr bù shūfu?",
          "text_translation": "Qayeringiz og'riyapti?",
          "text_translation_ru": "Где болит?",
          "speaker": "A",
          "words": [
            { "i": [0, 1], "p": "nǐ", "t": "sen", "tr": "ты", "h": 1, "l": 1 },
            { "i": [1, 3], "p": "nǎr", "t": "qayer", "tr": "где", "h": 1, "l": 8 },
            { "i": [3, 4], "p": "bù", "t": "emas", "tr": "не", "h": 1, "l": 2 },
            { "i": [4, 6], "p": "shūfu", "t": "qulay/yaxshi", "tr": "комфортно" }
          ]
        },
        {
          "id": "d12-s4",
          "text_original": "我头疼。",
          "pinyin": "Wǒ tóu téng.",
          "text_translation": "Boshim og'riyapti.",
          "text_translation_ru": "У меня болит голова.",
          "speaker": "B",
          "words": [
            { "i": [0, 1], "p": "wǒ", "t": "men", "tr": "я", "h": 1, "l": 1 },
            { "i": [1, 2], "p": "tóu", "t": "bosh", "tr": "голова" },
            { "i": [2, 3], "p": "téng", "t": "og'rimoq", "tr": "болеть" }
          ]
        },
        {
          "id": "d12-s5",
          "text_original": "发烧吗？",
          "pinyin": "Fāshāo ma?",
          "text_translation": "Issiqingiz bormi?",
          "text_translation_ru": "Температура есть?",
          "speaker": "A",
          "words": [
            { "i": [0, 2], "p": "fāshāo", "t": "isitma chiqmoq", "tr": "температура" },
            { "i": [2, 3], "p": "ma", "t": "-mi", "tr": "(вопрос)", "h": 1, "l": 2 }
          ]
        },
        {
          "id": "d12-s6",
          "text_original": "不发烧。",
          "pinyin": "Bù fāshāo.",
          "text_translation": "Yo'q, issiqim yo'q.",
          "text_translation_ru": "Нет, температуры нет.",
          "speaker": "B",
          "words": [
            { "i": [0, 1], "p": "bù", "t": "emas", "tr": "не", "h": 1, "l": 2 },
            { "i": [1, 3], "p": "fāshāo", "t": "isitma chiqmoq", "tr": "температура" }
          ]
        },
        {
          "id": "d12-s7",
          "text_original": "你去医院吗？",
          "pinyin": "Nǐ qù yīyuàn ma?",
          "text_translation": "Kasalxonaga borasizmi?",
          "text_translation_ru": "Пойдёшь в больницу?",
          "speaker": "A",
          "words": [
            { "i": [0, 1], "p": "nǐ", "t": "sen", "tr": "ты", "h": 1, "l": 1 },
            { "i": [1, 2], "p": "qù", "t": "bormoq", "tr": "идти", "h": 1, "l": 8 },
            { "i": [2, 4], "p": "yīyuàn", "t": "kasalxona", "tr": "больница", "h": 1, "l": 15 },
            { "i": [4, 5], "p": "ma", "t": "-mi", "tr": "(вопрос)", "h": 1, "l": 2 }
          ]
        },
        {
          "id": "d12-s8",
          "text_original": "不去，我想休息一下。",
          "pinyin": "Bù qù, wǒ xiǎng xiūxi yíxià.",
          "text_translation": "Yo'q, biroz dam olmoqchiman.",
          "text_translation_ru": "Нет, хочу немного отдохнуть.",
          "speaker": "B",
          "words": [
            { "i": [0, 1], "p": "bù", "t": "emas", "tr": "не", "h": 1, "l": 2 },
            { "i": [1, 2], "p": "qù", "t": "bormoq", "tr": "идти", "h": 1, "l": 8 },
            { "i": [3, 4], "p": "wǒ", "t": "men", "tr": "я", "h": 1, "l": 1 },
            { "i": [4, 5], "p": "xiǎng", "t": "xohlamoq", "tr": "хотеть", "h": 1, "l": 11 },
            { "i": [5, 7], "p": "xiūxi", "t": "dam olmoq", "tr": "отдыхать", "h": 1, "l": 5 },
            { "i": [7, 9], "p": "yíxià", "t": "biroz", "tr": "немного", "h": 1, "l": 12 }
          ]
        }
      ]
    }
