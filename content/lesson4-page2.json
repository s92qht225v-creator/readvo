      "audio_url": "https://miruwaeplbzfqmdwacsh.supabase.co/storage/v1/object/public/audio/HSK%201/HSK%204-2/dialogue.mp3",
      "image_url": "https://miruwaeplbzfqmdwacsh.supabase.co/storage/v1/object/public/images/HSK%201/HSK-4-2-1.jpg",
      "sentences": [
        {
          "id": "l4p2-t2-s1",
          "section": "text",
          "text_original": "我有两个哥哥，你呢？",
          "text_translation": "Mening ikkita akam bor, seninchi?",
          "text_translation_ru": "У меня два старших брата, а у тебя?",
          "pinyin": "Wǒ yǒu liǎng gè gēge, nǐ ne?",
          "speaker": "王一雪",
          "speakerName": "Wáng Yīxuě",
          "audio_url": "https://miruwaeplbzfqmdwacsh.supabase.co/storage/v1/object/public/audio/HSK%201/HSK%204-2/dialogue1.mp3"
        },
        {
          "id": "l4p2-t2-s2",
          "section": "text",
          "text_original": "我没有哥哥。",
          "text_translation": "Mening akam yo'q.",
          "text_translation_ru": "У меня нет старших братьев.",
          "pinyin": "Wǒ méiyǒu gēge.",
          "speaker": "杨同乐",
          "speakerName": "Yáng Tónglè",
          "audio_url": "https://miruwaeplbzfqmdwacsh.supabase.co/storage/v1/object/public/audio/HSK%201/HSK%204-2/dialogue2.mp3"
        },
        {
          "id": "l4p2-t2-s3",
          "section": "text",
          "text_original": "你家有几口人？",
          "text_translation": "Oilangda necha kishi bor?",
          "text_translation_ru": "Сколько человек в твоей семье?",
          "pinyin": "Nǐ jiā yǒu jǐ kǒu rén?",
          "speaker": "王一雪",
          "speakerName": "Wáng Yīxuě",
          "audio_url": "https://miruwaeplbzfqmdwacsh.supabase.co/storage/v1/object/public/audio/HSK%201/HSK%204-2/dialogue3.mp3"
        },
        {
          "id": "l4p2-t2-s4",
          "section": "text",
          "text_original": "我家有四口人，爸爸、妈妈、妹妹和我。",
          "text_translation": "Oilamda to'rt kishi bor: dadam, onam, singlim va men.",
          "text_translation_ru": "В моей семье четыре человека: папа, мама, младшая сестра и я.",
          "pinyin": "Wǒ jiā yǒu sì kǒu rén, bàba, māma, mèimei hé wǒ.",
          "speaker": "杨同乐",
          "speakerName": "Yáng Tónglè",
          "audio_url": "https://miruwaeplbzfqmdwacsh.supabase.co/storage/v1/object/public/audio/HSK%201/HSK%204-2/dialogue4.mp3"
        }
      ]
