    {
      "id": "l99p2-sec-text2",
      "type": "text",
      "heading": "",
      "subheading": "Dialog 2",
      "subheading_ru": "Диалог 2",
      "context": "Darsdan keyin. Li ustoz Azizni sinfdoshi Xiaomei bilan tanishtirmoqda.",
      "contextTranslation": "",
      "contextTranslation_ru": "После урока. Учитель Ли знакомит Азиза с одноклассницей Сяомэй.",
      "instruction": "朗读对话。Dialogni ovoz chiqarib o'qing.",
      "instruction_ru": "朗读对话。Прочитайте диалог вслух.",
      "image_url": "https://placehold.co/600x400/fef0e8/dd6b20?text=Li+laoshi+introduces\\nAziz+and+Xiaomei",
      "sentences": [
        {
          "id": "l99p2-t2-s1",
          "section": "text",
          "text_original": "Aziz，请认识一下，她叫小美。",
          "text_translation": "Aziz, tanishing, uning ismi Xiaomei.",
          "text_translation_ru": "Азиз, познакомься, её зовут Сяомэй.",
          "pinyin": "Aziz, qǐng rènshi yíxià, tā jiào Xiǎoměi.",
          "speaker": "李老师",
          "speakerName": "Lǐ lǎoshī"
        },
        {
          "id": "l99p2-t2-s2",
          "section": "text",
          "text_original": "你好，小美！认识你很高兴。",
          "text_translation": "Salom, Xiaomei! Tanishganimdan xursandman.",
          "text_translation_ru": "Привет, Сяомэй! Рад познакомиться.",
          "pinyin": "Nǐ hǎo, Xiǎoměi! Rènshi nǐ hěn gāoxìng.",
          "speaker": "Aziz",
          "speakerName": "Aziz"
        },
        {
          "id": "l99p2-t2-s3",
          "section": "text",
          "text_original": "你好，Aziz！我也很高兴。",
          "text_translation": "Salom, Aziz! Men ham xursandman.",
          "text_translation_ru": "Привет, Азиз! Я тоже очень рада.",
          "pinyin": "Nǐ hǎo, Aziz! Wǒ yě hěn gāoxìng.",
          "speaker": "小美",
          "speakerName": "Xiǎoměi"
        },
        {
          "id": "l99p2-t2-s4",
          "section": "text",
          "text_original": "谢谢老师！",
          "text_translation": "Rahmat, ustoz!",
          "text_translation_ru": "Спасибо, учитель!",
          "pinyin": "Xièxie lǎoshī!",
          "speaker": "Aziz",
          "speakerName": "Aziz"
        },
        {
          "id": "l99p2-t2-s5",
          "section": "text",
          "text_original": "不客气！",
          "text_translation": "Arzimaydi!",
          "text_translation_ru": "Не за что!",
          "pinyin": "Bú kèqi!",
          "speaker": "李老师",
          "speakerName": "Lǐ lǎoshī"
        }
      ]
    },
